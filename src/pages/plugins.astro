        bonusMultiplier: 2.0,
        notificationThreshold: "5"
      },
      update: "2024"
    }
  }
];
---

<Layout title="–ü–ª–∞–≥–∏–Ω—ã - Ultimate Rust Wiki">
  <main class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-12">
    <div class="bg-rust-gray rounded-lg p-8">
      <PageHeader 
        title="–ü–ª–∞–≥–∏–Ω—ã —Å–µ—Ä–≤–µ—Ä–∞" 
        subtitle="–£–Ω–∏–∫–∞–ª—å–Ω—ã–µ –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç–∏ –∏ –º–µ—Ö–∞–Ω–∏–∫–∏ –Ω–∞—à–µ–≥–æ —Å–µ—Ä–≤–µ—Ä–∞" 
      />
      
      <div id="plugins-list" class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">
        {plugins.map(plugin => (
          <div 
            class="bg-rust-dark/50 rounded-lg p-6 hover:bg-rust-dark/70 transition-all duration-300 cursor-pointer transform hover:scale-[1.02] group"
            data-plugin-id={plugin.id}
          >
            <div class="flex items-center space-x-3">
              <span class="text-3xl bg-rust-pink/10 w-12 h-12 flex items-center justify-center rounded-lg group-hover:animate-bounce">{plugin.icon}</span>
              <h3 class="text-xl font-semibold group-hover:text-rust-pink transition-colors">{plugin.name}</h3>
            </div>
            <p class="text-gray-300 mt-4">{plugin.description}</p>
          </div>
        ))}
      </div>

      <div id="plugin-details" class="hidden">
        {plugins.map(plugin => (
          <div id={`details-${plugin.id}`} class="hidden">
            <button class="mb-6 flex items-center space-x-2 text-rust-pink hover:text-rust-pink/80 transition-colors" onclick="showPluginsList()">
              <span>‚Üê</span>
              <span>–ù–∞–∑–∞–¥ –∫ —Å–ø–∏—Å–∫—É –ø–ª–∞–≥–∏–Ω–æ–≤</span>
            </button>
            
            <div class="bg-rust-dark/50 rounded-lg p-6">
              <div class="flex items-center space-x-3 mb-4">
                <span class="text-3xl bg-rust-pink/10 w-12 h-12 flex items-center justify-center rounded-lg">{plugin.icon}</span>
                <h3 class="text-xl font-semibold">{plugin.name}</h3>
              </div>
              
              <p class="text-gray-300 mb-4">{plugin.description}</p>
              
              <div class="space-y-2">
                <h4 class="text-sm font-semibold text-rust-pink mb-2">–í–æ–∑–º–æ–∂–Ω–æ—Å—Ç–∏:</h4>
                <ul class="list-disc list-inside space-y-1 text-gray-400">
                  {plugin.features.map(feature => (
                    <li>{feature}</li>
                  ))}
                </ul>
              </div>

              {plugin.id === "skin-system" && <SkinSystemDetails plugin={plugin} />}
              {plugin.id === "player-stats" && <PlayerStatsDetails plugin={plugin} />}
              {plugin.id === "moonshiner" && <MoonshinerDetails plugin={plugin} />}
              {plugin.id === "raid-block" && <RaidBlockDetails plugin={plugin} />}
              {plugin.id === "bank-heist" && <BankHeistDetails plugin={plugin} />}
              {plugin.id === "unique-ore" && <UniqueOreDetails plugin={plugin} />}
              {plugin.id === "bonuses-settings" && <BonusesAndSettingsDetails plugin={plugin} />}
              {plugin.id === "daily-rewards" && <DailyRewardsDetails plugin={plugin} />}
            </div>
          </div>
        ))}
      </div>
    </div>
  </main>
</Layout>

<script>
  function showPluginDetails(pluginId) {
    document.getElementById('plugins-list')?.classList.add('hidden');
    document.getElementById('plugin-details')?.classList.remove('hidden');
    
    // –°–∫—Ä—ã—Ç—å –≤—Å–µ –¥–µ—Ç–∞–ª–∏ –ø–ª–∞–≥–∏–Ω–æ–≤
    document.querySelectorAll('[id^="details-"]').forEach(el => {
      el.classList.add('hidden');
    });
    
    // –ü–æ–∫–∞–∑–∞—Ç—å –¥–µ—Ç–∞–ª–∏ –≤—ã–±—Ä–∞–Ω–Ω–æ–≥–æ –ø–ª–∞–≥–∏–Ω–∞
    document.getElementById(`details-${pluginId}`)?.classList.remove('hidden');
  }

  function showPluginsList() {
    document.getElementById('plugins-list')?.classList.remove('hidden');
    document.getElementById('plugin-details')?.classList.add('hidden');
  }

  // –î–æ–±–∞–≤–∏—Ç—å –æ–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ –∫–ª–∏–∫–æ–≤ –∫ –∫–∞—Ä—Ç–æ—á–∫–∞–º –ø–ª–∞–≥–∏–Ω–æ–≤
  document.querySelectorAll('[data-plugin-id]').forEach(card => {
    card.addEventListener('click', (e) => {
      const target = e.currentTarget;
      const pluginId = target.getAttribute('data-plugin-id');
      if (pluginId) {
        showPluginDetails(pluginId);
      }
    });
  });
</script>